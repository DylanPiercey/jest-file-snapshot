    "lint": "eslint .",
    "release": "release-it"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@types/jest": "^23.3.10",
    "@types/jest-diff": "^20.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^5.9.0",
    "eslint-config-satya164": "^2.0.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "release-it": "^8.2.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "filenamify": "^2.1.0",
    "jest-diff": "^23.6.0",
    "mkdirp": "^0.5.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "watchPathIgnorePatterns": [
      "(__fixtures__|__file_snapshots__)"
    ]
  },
  "husky": {
    "hooks": {
