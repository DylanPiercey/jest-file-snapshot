{
  "name": "jest-file-snapshot",
  "version": "0.3.0",
  "description": "",
  "keywords": [
    "test",
    "jest",
    "snapshot"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "types": "./index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satya164/jest-file-snapshot.git"
  },
  "author": "Satyajit Sahoo <satyajit.happy@gmail.com> (https://github.com/satya164/)",
